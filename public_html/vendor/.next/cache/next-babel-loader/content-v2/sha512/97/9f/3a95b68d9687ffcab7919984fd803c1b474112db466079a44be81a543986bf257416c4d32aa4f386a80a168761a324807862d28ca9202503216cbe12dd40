en(false);\n  };\n  // creates the links that appear in the left menu / Sidebar\n  const createLinks = (routes) => {\n    return routes.map((prop, key) => {\n      return (\n\n        (prop.sprate)? \n          <span key={key}>\n            <hr className=\"my-3\" />\n            <h6 className=\"navbar-heading text-muted ml-2\">{prop.title}</h6>\n          </span>\n        :\n        <NavItem key={key} active={activeRoute(prop.path)}>\n          <Link href={prop.path}>\n            <NavLink\n              href=\"#pablo\"\n              active={activeRoute(prop.path)}\n              onClick={closeCollapse}\n            >\n              <i className={prop.icon} />\n              {prop.name}\n            </NavLink>\n          </Link>\n        </NavItem>\n      );\n    });\n  };\n  const { routes, logo } = props;\n  let navbarBrand = (\n    <NavbarBrand href={logo.innerLink} className=\"pt-0\">\n      <img alt={logo.imgAlt} className=\"navbar-brand-img\" src={logo.imgSrc} />\n    </NavbarBrand>\n  );\n  return (\n    <Navbar\n      className=\"navbar-vertical fixed-left navbar-light bg-white\"\n      expand=\"md\"\n      id=\"sidenav-main\"\n    >\n      <Container fluid>\n        {/* Toggler */}\n        <button\n          className=\"navbar-toggler\"\n          type=\"button\"\n          onClick={toggleCollapse}\n        >\n          <span className=\"navbar-toggler-icon\" />\n        </button>\n        {/* Brand */}\n        {logo && logo.innerLink ? (\n          <Link href={logo.innerLink}>\n            <span>{navbarBrand}</span>\n          </Link>\n        ) : null}\n        {logo && logo.outterLink ? (\n          <a href={logo.innerLink} target=\"_blank\">\n            {navbarBrand}\n          </a>\n        ) : null}\n        {/* User */}\n        <Nav className=\"align-items-center d-md-none\">\n          <UncontrolledDropdown nav>\n            <DropdownToggle nav className=\"nav-link-icon\">\n              <i className=\"ni ni-bell-55\" />\n            </DropdownToggle>\n            <DropdownMenu\n              aria-labelledby=\"navbar-default_dropdown_1\"\n              className=\"dropdown-menu-arrow\"\n              right\n            >\n              <DropdownItem>Action</DropdownItem>\n              <DropdownItem>Another action</DropdownItem>\n              <DropdownItem divider />\n              <DropdownItem>Something else here</DropdownItem>\n            </DropdownMenu>\n          </UncontrolledDropdown>\n          <UncontrolledDropdown nav>\n            <DropdownToggle nav>\n              <Media className=\"align-items-center\">\n                <span className=\"avatar avatar-sm rounded-circle\">\n                  <img\n                    alt=\"...\"\n                    src={require(\"assets/img/theme/team-1-800x800.jpg\")}\n                  />\n                </span>\n              </Media>\n            </DropdownToggle>\n            <DropdownMenu className=\"dropdown-menu-arrow\" right>\n              <DropdownItem className=\"noti-title\" header tag=\"div\">\n                <h6 className=\"text-overflow m-0\">Welcome!</h6>\n              </DropdownItem>\n              <Link href=\"/profile\">\n                <DropdownItem>\n                  <i className=\"ni ni-single-02\" />\n                  <span>My profile</span>\n                </DropdownItem>\n              </Link>\n              <Link href=\"/profile\">\n                <DropdownItem>\n                  <i className=\"ni ni-settings-gear-65\" />\n                  <span>Settings</span>\n                </DropdownItem>\n              </Link>\n              <Link href=\"/profile\">\n                <DropdownItem>\n                  <i className=\"ni ni-calendar-grid-58\" />\n                  <span>Activity</span>\n                </DropdownItem>\n              </Link>\n              <Link href=\"/profile\">\n                <DropdownItem>\n                  <i className=\"ni ni-support-16\" />\n                  <span>Support</span>\n           