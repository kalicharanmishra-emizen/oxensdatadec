    } \n}\nexport const updateDiscount = (value) => async dispatch =>{\n    try {\n        const res = await callApi('post','/discount/update',value)\n        dispatch(apiSuccess(res.data))\n        dispatch(listing(res.data))\n    } catch (e) {\n        dispatch(apiFail(e))\n    } \n}"]},"metadata":{},"sourceType":"module"}