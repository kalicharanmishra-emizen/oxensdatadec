                     <FormGroup>\n                                        <label\n                                            className=\"form-control-label\"\n                                        >\n                                            Title\n                                        </label>\n                                            <FormInputCom\n                                                type=\"text\"\n                                                name=\"title\"\n                                                placeholder=\"Enter Title\"\n                                                optionData={[]}\n                                                validator={\n                                                    [\n                                                        {\n                                                            property:validator.isEmpty,\n                                                            message:\"Category title required\",\n                                                            isNot:false\n                                                        },\n                                                    ]\n                                                }\n                                                getValue={getValue}\n                                                defValue={formSubmit.title}\n                                                submitValidate={submitValidate}\n                                            />\n                                        </FormGroup>\n                                    </Col>\n                                    </Row>\n                                    <Row>\n                                    <Col lg=\"12\">\n                                        <Button\n                                            className=\"float-right\"\n                                            color=\"primary\"\n                                            type=\"submit\"\n                                        >\n                                            Save\n                                        </Button>\n                                        <Button\n                                            className=\"float-right mr-2\"\n                                            color=\"secondary\"\n                                            type=\"button\"\n                                            onClick={\n                                                (e)=>{\n                                                    e.preventDefault()\n                                                    router.back()\n                                                }\n                                            }\n                                        >\n                                            Back\n                                        </Button>\n                                    </Col>\n                                    </Row>\n                                </div>\n                                </Form>\n                            </CardBody>\n                        </Card>\n                    </Col>\n                </Row>\n            </Container>\n        </>\n    )\n}\nCreate.layout=Admin\nexport default Create\n"]},"metadata":{},"sourceType":"module"}