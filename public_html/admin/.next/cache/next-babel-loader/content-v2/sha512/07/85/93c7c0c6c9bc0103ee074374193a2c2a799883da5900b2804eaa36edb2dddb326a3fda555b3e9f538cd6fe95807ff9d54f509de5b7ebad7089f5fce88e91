data={paginationData}\n                                            pagFun={pageChange}\n                                        />\n                                    :null        \n                                }\n                    \n                            </nav>\n                            <div>\n                                <Button\n                                    className=\"float-right\"\n                                    color=\"secondary\"\n                                    type=\"button\"\n                                    onClick={\n                                        (e)=>{\n                                            e.preventDefault()\n                                            router.push(`/vendor/${store}`)\n                                        }\n                                    }\n                                >\n                                    Back\n                                </Button>\n                            </div>\n                        </CardFooter>\n                    </Card>\n                    </Col>\n                </Row>\n            </Container>\n        </>\n    )\n}\nIndex.layout=Admin\nexport default Index\n"]},"metadata":{},"sourceType":"module"}